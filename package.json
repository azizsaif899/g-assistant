{
  "name": "azizsys",
  "version": "6.3.0",
  "description": "ðŸš€ AzizSys - Ù†Ø¸Ø§Ù… Ø¥Ø¯Ø§Ø±Ø© Ø°ÙƒÙŠ Ù…ØªÙƒØ§Ù…Ù„ Ù…Ø¯Ø¹ÙˆÙ… Ø¨Ø§Ù„Ø°ÙƒØ§Ø¡ Ø§Ù„ØµÙ†Ø§Ø¹ÙŠ",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "build": "npm run lint && npm run test",
    "full-build": "npm run clean && npm run build && npm run deploy-prep",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --fix",
    "clean": "rimraf dist/ gas_ready/",
    "deploy-prep": "mkdir -p gas_ready && cp -r src/* gas_ready/",
    "clasp:push": "clasp push",
    "clasp:deploy": "clasp deploy",
    "docs:build": "node scripts/build-docs.js",
    "docs:serve": "http-server docs/ -p 8080",
    "health-check": "node scripts/health-check.js",
    "system-doctor": "node scripts/system-doctor.js"
  },
  "keywords": [
    "google-apps-script",
    "artificial-intelligence",
    "gemini-ai",
    "automation",
    "google-sheets",
    "intelligent-assistant",
    "arabic",
    "financial-analysis",
    "data-analysis",
    "code-review"
  ],
  "author": "AzizSys Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/azizsys5.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/azizsys5/issues"
  },
  "homepage": "https://github.com/your-username/azizsys5#readme",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "eslint": "^8.57.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.15.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,gs}",
      "!src/**/*.test.{js,gs}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "googleappsscript/googleappsscript": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "googleappsscript"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "prefer-const": "error",
      "no-var": "error"
    }
  }
}
