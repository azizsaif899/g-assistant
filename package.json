{
  "name": "g-assistant-auto-fix",
  "version": "1.0.0",
  "description": "Advanced Auto-Fix System for G-Assistant with AI-powered code analysis and repair",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "dashboard:serve": "node docs/6_fixing/dashboard/server.js",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cron": "^3.1.0",
    "dotenv": "^16.3.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "auto-fix",
    "ai",
    "code-analysis",
    "automation",
    "typescript",
    "gemini"
  ],
  "author": "G-Assistant Team",
  "license": "MIT"
}