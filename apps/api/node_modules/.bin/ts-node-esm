#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._39aab30a9b23b7cc8c18d1e7efa2b8cc/node_modules/ts-node/dist/node_modules:/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._39aab30a9b23b7cc8c18d1e7efa2b8cc/node_modules/ts-node/node_modules:/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._39aab30a9b23b7cc8c18d1e7efa2b8cc/node_modules:/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._39aab30a9b23b7cc8c18d1e7efa2b8cc/node_modules/ts-node/dist/node_modules:/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._39aab30a9b23b7cc8c18d1e7efa2b8cc/node_modules/ts-node/node_modules:/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._39aab30a9b23b7cc8c18d1e7efa2b8cc/node_modules:/mnt/e/azizsys5/g-assistant-nx/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-esm.js" "$@"
fi
