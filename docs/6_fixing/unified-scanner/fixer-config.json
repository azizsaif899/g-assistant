{
  "enabled": true,
  "fixes": {
    "removeConsoleLog": true,
    "removeTrailingSpaces": true,
    "removeExtraEmptyLines": true,
    "fixSpacesAroundBrackets": true
  },
  "excludeFiles": [
    "package-lock.json",
    "node_modules",
    ".git",
    "dist",
    "build",
    ".next",
    "coverage",
    "*.min.js",
    "*.bundle.js"
  ],
  "supportedExtensions": [
    ".js",
    ".ts",
    ".jsx",
    ".tsx",
    ".css",
    ".scss",
    ".vue"
  ],
  "createBackup": true,
  "maxFileSize": 1048576
}