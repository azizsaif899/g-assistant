{
  "assistants": {
    "copilot": {
      "name": "GitHub Copilot",
      "responsibilities": ["ui", "frontend", "testing"],
      "priority": 2,
      "auto_push": false,
      "branch_prefix": "copilot",
      "notification_webhook": "",
      "file_patterns": [
        "src/UI/**",
        "frontend/**", 
        "*.html",
        "*.css",
        "*.jsx",
        "tests/**"
      ]
    },
    "gemini": {
      "name": "Google Gemini",
      "responsibilities": ["ai", "agents", "analysis"],
      "priority": 1,
      "auto_push": true,
      "branch_prefix": "gemini",
      "notification_webhook": "",
      "file_patterns": [
        "src/AI/**",
        "src/agents/**",
        "20_ai/**",
        "25_ai_agents/**",
        "*.ai.js"
      ]
    },
    "human": {
      "name": "Human Developer",
      "responsibilities": ["critical", "config", "architecture"],
      "priority": 0,
      "auto_push": true,
      "branch_prefix": "main",
      "notification_webhook": "",
      "file_patterns": [
        "config/**",
        "*.json",
        "README.md",
        "package.json",
        "deployment/**"
      ]
    }
  },
  "sync_rules": {
    "conflict_resolution": "human_priority",
    "auto_merge": false,
    "require_review": true,
    "notification_delay": 5,
    "max_concurrent_changes": 3
  },
  "git_settings": {
    "default_branch": "master",
    "commit_message_template": "{emoji} {assistant}: {description} [{timestamp}]",
    "auto_pull_before_push": true,
    "create_backup_branch": true
  },
  "webhooks": {
    "enabled": false,
    "discord_webhook": "",
    "slack_webhook": "",
    "teams_webhook": ""
  },
  "change_types": {
    "ui": {
      "emoji": "üé®",
      "priority": "medium",
      "requires_review": false
    },
    "ai": {
      "emoji": "ü§ñ", 
      "priority": "high",
      "requires_review": true
    },
    "config": {
      "emoji": "‚öôÔ∏è",
      "priority": "high", 
      "requires_review": true
    },
    "critical": {
      "emoji": "üö®",
      "priority": "urgent",
      "requires_review": true,
      "immediate_notification": true
    },
    "docs": {
      "emoji": "üìö",
      "priority": "low",
      "requires_review": false
    },
    "test": {
      "emoji": "üß™",
      "priority": "medium",
      "requires_review": false
    }
  }
}