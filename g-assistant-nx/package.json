{
  "name": "@azizsys/g-assistant-nx",
  "version": "2.0.0",
  "description": "AzizSys AI Assistant v2.0 - النقلة النوعية الكبرى",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "nx build",
    "dev": "nx serve",
    "start": "nx serve",
    "lint": "nx lint",
    "test": "nx test",
    "test:unit": "vitest run tests/unit --reporter=verbose",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "test:e2e": "playwright test tests/e2e",
    "test:performance": "vitest run tests/performance --reporter=verbose",
    "test:security": "vitest run tests/security --reporter=verbose",
    "test:all": "tsx tests/test-scripts/run-all-tests.ts",
    "test:setup": "tsx tests/test-scripts/setup-test-environment.ts",
    "test:cleanup": "tsx tests/test-scripts/setup-test-environment.ts --cleanup",
    "test:report": "tsx tests/test-scripts/generate-test-report.ts",
    "test:demo-report": "tsx tests/test-scripts/demo-test-report.ts",
    "test:crm": "vitest run tests/unit/crm-system.test.ts tests/integration/crm-integration.test.ts --reporter=verbose",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "npm run test:setup && npm run test:all && npm run test:cleanup",
    "dev:api": "nx serve api",
    "dev:admin-dashboard": "nx serve admin-dashboard",
    "dev:web-chatbot": "nx serve web-chatbot",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:admin-dashboard\" \"npm run dev:web-chatbot\"",
    "build:all": "nx build api && nx build admin-dashboard && nx build web-chatbot",
    "start:test": "NODE_ENV=test nx serve api --port=3001",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "NODE_ENV=test prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "activate:cfo-agent": "echo 'CFO Agent activated'",
    "activate:developer-agent": "echo 'Developer Agent activated'",
    "activate:database-manager": "echo 'Database Manager activated'",
    "activate:operations-agent": "echo 'Operations Agent activated'",
    "activate:general-agent": "echo 'General Agent activated'",
    "activate:smart-mode": "echo 'Smart Mode activated'",
    "activate:iterative-mode": "echo 'Iterative Mode activated'",
    "activate:analysis-mode": "echo 'Analysis Mode activated'",
    "system:health": "tsx scripts/health-check-v2.js",
    "dashboard": "nx serve admin-dashboard",
    "auto:enhanced": "tsx scripts/auto-fix-v2.js",
    "october:build": "nx build october-implementation",
    "gemini:build": "nx build gemini-research-agent",
    "gemini:backend": "nx serve gemini-backend"
  },
  "devDependencies": {
    "@nx/eslint": "19.8.4",
    "@nx/eslint-plugin": "19.8.4",
    "@nx/jest": "19.8.4",
    "@nx/js": "19.8.4",
    "@nx/node": "19.8.4",
    "@nx/react": "19.8.4",

    "@nx/vite": "19.8.4",
    "@nx/web": "19.8.4",
    "@nx/workspace": "19.8.4",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitest/coverage-v8": "^1.0.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "msw": "^2.0.0",
    "nx": "19.8.4",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.8",
    "@nestjs/core": "^10.2.8",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/testing": "^10.2.8",
    "@prisma/client": "^5.6.0",
    "prisma": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.0"
  },
  "nx": {
    "includedScripts": []
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azizsys/g-assistant-nx.git"
  },
  "keywords": [
    "ai",
    "assistant",
    "crm",
    "automation",
    "typescript",
    "react",
    "nestjs"
  ],
  "author": "AzizSys Team",
  "license": "MIT"
}